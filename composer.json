{
    "name": "dhii/sql-interface",
    "description": "Interfaces for SQL resource abstraction.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dhii Team",
            "email": "development@dhii.co"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.3 | ^7.0",
        "dhii/expression-interface": "^0.2",
        "dhii/expression-renderer-interface": "^0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "ptrofimov/xpmock": "^1.1",
        "dhii/php-cs-fixer-config": "dev-php-5.3",
        "codeclimate/php-test-reporter": "<=0.3.2",
        "dhii/stringable-interface": "^0.1"
    },
    "autoload": {
        "psr-4": {
            "Dhii\\Storage\\Resource\\Sql\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dhii\\Storage\\Resource\\Sql\\UnitTest\\": "test/unit",
            "Dhii\\Storage\\Resource\\Sql\\FuncTest\\": "test/functional"
        }
    },
    "scripts": {
        "test": "phpunit",
        "csfix": "php-cs-fixer fix -vvv"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.1.x-dev"
        }
    }
}
